/* components/clickItem/clickItem.wxss */
.container{
  display: flex;
  justify-content: space-around;
  flex-wrap:wrap;
  box-sizing: border-box;
  padding-top:120rpx;
}
.item{
  width: 40%;
  height: 260rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 60rpx
}
.item-img{
  width: 100%;
  height: 220rpx;
  border: 1px solid grey;
  padding-bottom: 40rpx;
  box-sizing: border-box;
}