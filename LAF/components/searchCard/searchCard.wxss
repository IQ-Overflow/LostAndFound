/* components/searchCard/searchCard.wxss */
.input-area{
    width: 80%;
    left: 0;
    right: 0;
    margin: 10% auto;
}
.input{
    display: flex;
    justify-content: space-around;
    margin-bottom: 20px;
}
input, picker{
    border: solid 2px #f3f3f3;
    border-radius: 3px;
    width: 70%;
    height: 29px;
    line-height: 25px;
    padding-left: 8px;
    box-sizing: border-box;
}
.search{
    margin-bottom: 10%;
}